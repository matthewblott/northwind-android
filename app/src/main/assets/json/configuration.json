{
  "settings": {
    "screenshots_enabled": true
  },
  "rules": [
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "context": "default",
        "uri": "northwind://fragment/web",
        "fallback_uri": "northwind://fragment/web",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "^$",
        "^/$"
      ],
      "properties": {
        "uri": "northwind://fragment/web/home",
        "presentation": "replace_root"
      }
    },
    {
      "patterns": [
        "/numbers$"
      ],
      "properties": {
        "uri": "northwind://fragment/numbers",
        "title": "Numbers"
      }
    }
  ]
}
